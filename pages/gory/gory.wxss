.gory{
  font-style: "PingFangSC-Regular"
}
.gory_img{
  width: 100%;
  height: 340rpx;
}
.img{
  width: 100%;
  height: 100%;
  transform: scale(1.05)
}
.gory_list{
  height: 120rpx;
  line-height: 120rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 2px solid rgba(162,162,162,0.64);
}
.list{
  flex: 1;
  margin: 0 10px 4px;
  text-align: center;
}
.list_text{
  display: block;
  width: 100%;
  height: 100%;
  font-size: 16px;
}
.selected{
  border-bottom: 4px solid rgb(183,11,2);
}
.gory_container{
  padding: 20rpx;
}
.edit{
  font-size: 16px;
  color: rgb(183,11,2);
}
.file_container{
  margin-top:10rpx;
  line-height: 80rpx;
  border-bottom: 1px solid rgb(210,210,218);
}
.edit_img{
  float: right;
}
.downImg{
  margin-left: 20rpx;
  width: 50rpx;
  height: 50rpx;
}